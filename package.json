{
  "name": "react-redux-isolate",
  "version": "1.0.0",
  "description": "Isolate Redux apps to sandboxed state subtrees",
  "homepage": "https://github.com/Trimma/react-redux-isolate",
  "author": "Trimma AB",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "main": "lib/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "lib",
    "es",
    "src"
  ],
  "scripts": {
    "clean": "rimraf lib es",
    "lint": "eslint src",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build": "npm run build:commonjs && npm run build:es",
    "prepublish": "npm run clean && npm run lint && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com:Trimma/react-redux-isolate.git"
  },
  "keywords": [
    "redux",
    "reducer",
    "state",
    "isolate",
    "isolation",
    "sandbox",
    "functional",
    "elm",
    "sub-tree",
    "cycle"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "cross-env": "^1.0.7",
    "eslint": "^2.9.0",
    "eslint-plugin-react": "^5.0.1",
    "invariant": "^2.2.1",
    "react": "^0.14.7",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "rimraf": "^2.5.2"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0-0",
    "redux": "^2.0.0 || ^3.0.0",
    "react-redux": "^3.0.0 || ^4.0.0"
  }
}
