{
  "name": "react-redux-isolate",
  "version": "1.0.4",
  "description": "Isolate Redux apps to sandboxed state subtrees",
  "homepage": "https://github.com/Trimma/react-redux-isolate",
  "author": "Trimma AB",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "main": "lib/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "lib",
    "es",
    "src"
  ],
  "scripts": {
    "clean": "rimraf lib es",
    "lint": "eslint src test",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build": "npm run build:commonjs && npm run build:es",
    "prepublish": "npm run clean && npm run lint && npm run test && npm run build",
    "test": "mocha --compilers js:babel-register --recursive --require ./test/setup.js",
    "test:watch": "npm test -- --watch",
    "test:cov": "babel-node ./node_modules/isparta/bin/isparta cover ./node_modules/mocha/bin/_mocha -- --recursive"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com:Trimma/react-redux-isolate.git"
  },
  "keywords": [
    "redux",
    "reducer",
    "state",
    "isolate",
    "isolation",
    "sandbox",
    "functional",
    "elm",
    "sub-tree",
    "cycle"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-eslint": "7.1.1",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "babel-register": "6.18.0",
    "cross-env": "3.1.4",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "3.12.2",
    "eslint-plugin-react": "6.8.0",
    "expect": "1.20.2",
    "invariant": "2.2.2",
    "isparta": "4.0.0",
    "jsdom": "^11.5.1",
    "mocha": "3.2.0",
    "react": "16.3.0",
    "react-dom": "16.3.0",
    "react-redux": "5.0.6",
    "redux": "3.7.2",
    "redux-thunk": "2.1.0",
    "rimraf": "2.5.4"
  },
  "dependencies": {
    "prop-types": "^15.6.0"
  },
  "peerDependencies": {
    "react": "^16.3.0-0",
    "react-redux": "^3.0.0 || ^4.0.0 || ^5.0.0",
    "redux": "^2.0.0 || ^3.0.0"
  }
}
